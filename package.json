{
  "name": "nepali-datepicker-reactjs",
  "version": "1.0.0",
  "description": "Nepali Datepicker (Bikram Sambat) as a ReactJS component",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "rollup -c",
    "watch": "rollup -c --watch",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/puncoz-official/nepali-datepicker-reactjs.git"
  },
  "keywords": [
    "nepali",
    "datepicker",
    "bikram",
    "sambat"
  ],
  "author": "Puncoz Nepal <info@puncoz.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/puncoz-official/nepali-datepicker-reactjs/issues"
  },
  "homepage": "https://github.com/puncoz-official/nepali-datepicker-reactjs#readme",
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.8.5",
    "@types/react-test-renderer": "^16.9.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts-ts": "^4.0.8",
    "react-test-renderer": "^16.9.0",
    "rollup": "^1.19.4",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.22.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "files": [
    "dist/**/*"
  ]
}
